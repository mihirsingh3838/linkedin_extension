{
    "manifest_version": 3,
    "name": "LinkedIn Profile Picture Replacer",
    "version": "1.0",
    "description": "Replaces profile pictures on LinkedIn homepage with a chosen image (local)",
    "web_accessible_resources":[
      {
        "resources": ["download.jpeg"],
        "matches": ["https://www.linkedin.com/*"]
      }
    ],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  